---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>http://www.yoncise.com/blog/</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d"}}</lastmod>
        <changefreq>daily</changefreq>
        <priority>1.0</priority>
    </url>
    <url>
        <loc>http://www.yoncise.com/blog/about/</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d"}}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.8</priority>
    </url>
    <url>
        <loc>http://www.yoncise.com/blog/category/</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d"}}</lastmod>
        <changefreq>daily</changefreq>
        <priority>0.7</priority>
    </url>
    <url>
        <loc>http://www.yoncise.com/blog/link/</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d"}}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.6</priority>
    </url>
    {% for post in site.posts %}
    <url>
        <loc>http://www.yoncise.com{{ post.url }}</loc>
        <lastmod>
            {% if post.modified %}
            {{ post.modified | date: "%Y-%m-%d" }}
            {% else %}
            {{ post.date | date: "%Y-%m-%d" }}
            {% endif %}
        </lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.5</priority>
    </url>
    {% endfor %}
</urlset>
